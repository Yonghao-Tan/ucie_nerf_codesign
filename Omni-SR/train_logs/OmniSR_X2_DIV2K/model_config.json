{
    "version": "OmniSR_X2_DIV2K",
    "tag": "OmniSR",
    "phase": "train",
    "cuda": 6,
    "ckpt": 74,
    "experiment_description": "DIV2K X2",
    "train_yaml": "OmniSR_X2_DIV2K.yaml",
    "gamma": 0.01,
    "logger": "tensorboard",
    "local_rank": 0,
    "train_script_name": "common",
    "module_script_name": "OmniSR",
    "class_name": "OmniSR",
    "feature_num": 64,
    "module_params": {
        "upsampling": 2,
        "kernel_size": 17,
        "res_num": 5,
        "block_num": 1,
        "bias": true,
        "block_script_name": "OSA",
        "block_class_name": "OSA_Block",
        "window_size": 8,
        "pe": true,
        "ffn_bias": true
    },
    "total_epoch": 1000,
    "dataloader": "DIV2K_memory",
    "dataset_name": "DIV2K",
    "batch_size": 64,
    "random_seed": 1234,
    "dataset_params": {
        "lr_patch_size": 64,
        "degradation": "bicubic",
        "image_scale": 2,
        "subffix": "png",
        "dataloader_workers": 6,
        "dataset_enlarge": 64,
        "color_jitter": false,
        "color_config": {
            "brightness": 0.02,
            "contrast": 0.02,
            "saturation": 0.02,
            "hue": 0.02
        },
        "enable_reshuffle": false
    },
    "eval_dataloader": "memory",
    "eval_dataset_name": "urban100",
    "eval_batch_size": 1,
    "lr_decay": 0.5,
    "lr_decay_step": [
        250,
        500,
        750,
        1000
    ],
    "lr_decay_enable": true,
    "optim_type": "AdamW",
    "optim_config": {
        "lr": 0.0005,
        "betas": [
            0.9,
            0.999
        ],
        "weight_decay": 0.0001
    },
    "l1_weight": 30.0,
    "perceptual": {
        "layer_weights": {
            "conv5_4": 1
        },
        "vgg_type": "vgg19",
        "use_input_norm": true,
        "perceptual_weight": 1.0,
        "criterion": "l1"
    },
    "log_step": 200,
    "sample_step": 200,
    "model_save_epoch": 1,
    "use_tensorboard": true,
    "checkpoint_names": {
        "generator_name": "OmniSR"
    },
    "log_root_path": "./train_logs",
    "project_root": "./train_logs/OmniSR_X2_DIV2K",
    "project_summary": "./train_logs/OmniSR_X2_DIV2K/summary",
    "project_checkpoints": "./train_logs/OmniSR_X2_DIV2K/checkpoints",
    "project_samples": "./train_logs/OmniSR_X2_DIV2K/samples",
    "project_scripts": "./train_logs/OmniSR_X2_DIV2K/scripts",
    "reporter_path": "./train_logs/OmniSR_X2_DIV2K/OmniSR_X2_DIV2K_report"
}